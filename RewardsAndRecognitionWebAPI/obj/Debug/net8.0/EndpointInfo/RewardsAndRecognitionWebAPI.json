        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "isActive": {
            "type": "boolean"
          },
          "isDeleted": {
            "type": "boolean"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "nominations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Nomination"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
