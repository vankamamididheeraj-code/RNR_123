        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "nominatorId": {
            "type": "string",
            "nullable": true
          },
          "nominator": {
            "$ref": "#/components/schemas/User"
          },
          "nomineeId": {
            "type": "string",
            "nullable": true
          },
          "nominee": {
            "$ref": "#/components/schemas/User"
          },
          "categoryId": {
            "type": "string",
            "format": "uuid"
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "achievements": {
            "type": "string",
